#include <stdio.h>

int main() {
    int numero, i;

    printf("Digite um numero para ver a tabuada: ");
    scanf("%d", &numero); // Ler o número do usuário

    printf("\nTabuada do %d:\n", numero);

    // Loop de 1 a 10 para a tabuada
    for (i = 1; i <= 10; i++) {
        // Multiplicar o número pelo contador e exibir o resultado
        printf("%d x %d = %d\n", numero, i, numero * i);
    }

    return 0;
}
